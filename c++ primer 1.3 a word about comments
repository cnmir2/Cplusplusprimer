c++ primer 1.3 a word about comments

include <iostream>
/*
*simple main function
*
*
*read two numbers and writes their sum
*/

int main()

{
//prompt user to enter two numbers 

std::cout <<"enter two numbers:" << std::endle;

int v1= 0,v2=0; //variables to hold the input we read 

std::cin >> v1 >> v2;// read input

std::<< "the sum of " << v1 << "and" << v2 << "is" << v1+v2 << std::endl;

return 0;


}

